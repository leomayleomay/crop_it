<h1>New avatar</h1>

<% form_for(@avatar, :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :avatar %><br>
    <%= f.file_field :uploaded_data %>
  </p>

  <p>
    <%= f.submit 'Create', :disable_with => "Creating ..." %>
  </p>
<% end %>

<%= link_to 'Back', avatars_path %>
